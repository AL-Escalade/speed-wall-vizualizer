{
  "name": "voie-vitesse",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "generate": "tsx src/cli.ts",
    "generate:base": "tsx src/cli.ts generate -c data/base.json -o output/base.svg",
    "generate:ifsc": "tsx src/cli.ts generate -c data/ifsc.json -o output/ifsc.svg",
    "generate:u11-training": "tsx src/cli.ts generate -c data/u11-u13-training.json -o output/u11-u13-training.svg",
    "generate:u11-comp": "tsx src/cli.ts generate -c data/u11-u13-comp.json -o output/u11-u13-comp.svg",
    "generate:u15": "tsx src/cli.ts generate -c data/u15.json -o output/u15.svg",
    "generate:project": "tsx src/cli.ts generate -c data/project.json -o output/project.svg",
    "build": "tsc",
    "test": "vitest"
  },
  "author": "",
  "license": "ISC",
  "description": "SVG generator for speed climbing route visualization",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pdfkit": "^0.17.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "optionalDependencies": {
    "pdfkit": "^0.15.0",
    "sharp": "^0.33.0"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.11"
  }
}
