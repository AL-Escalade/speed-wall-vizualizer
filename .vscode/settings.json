{
  "editor.codeActionsOnSave": {
    "source.fixAll.oxc": "explicit"
  },
  "json.schemas": [
    {
      "fileMatch": ["data/*.json", "!data/routes/*.json"],
      "url": "./schemas/config.schema.json"
    },
    {
      "fileMatch": ["data/routes/*.json"],
      "url": "./schemas/route.schema.json"
    }
  ]
}
